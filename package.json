{
	"name": "andrew-baisden-portfolio-2018",
	"jest": {
		"transform": {
			"^.+\\.js$": "babel-jest",
			"^.+\\.jsx$": "babel-jest"
		},
		"transformIgnorePatterns": [
			"/node_modules/(?!react-flickity-component/src/index).+\\.js$"
		],
		"modulePaths": [
			"__mocks__"
		],
		"moduleNameMapper": {
			"\\.(css|sass|scss)$": "styleMock.js",
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|mov|wav|mp3|m4a|aac|oga)$": "fileMock.js"
		},
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/build/"
		]
	},
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"concurrently": "^3.5.1",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.13.0",
		"jest-config": "^24.8.0",
		"jest-svg-transformer": "^1.0.0",
		"normalize.css": "^7.0.0",
		"prop-types": "^15.6.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-flickity-component": "^1.1.3",
		"react-helmet": "^5.2.0",
		"react-router-dom": "^4.2.2",
		"react-scripts": "^3.0.1",
		"react-test-renderer": "^16.8.6",
		"styled-components": "^3.1.4",
		"stylus": "^0.54.5"
	},
	"scripts": {
		"start": "react-scripts start",
		"styles": "stylus -w ./src/css/App.styl -o ./src/css/App.css",
		"dev": "concurrently \"npm run start \" \"npm run styles\"",
		"watch": "run-p start styles",
		"build": "react-scripts build",
		"test": "jest --watch",
		"test:react": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	},
	"devDependencies": {
		"@babel/core": "^7.4.4",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^24.8.0",
		"babel-plugin-styled-components": "^1.10.0",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-react": "^7.6.1",
		"npm-run-all": "^4.1.2",
		"prettier": "1.10.2"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
